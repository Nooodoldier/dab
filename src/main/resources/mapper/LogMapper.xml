<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bling.dab.dao.LogMapper">
    <insert id="saveLog" parameterType="com.bling.dab.domain.Log">
        insert into t_log(id,modelName,action,description,ip,createTime,success,message,userId,className,methodName,methodArgs)
         values(#{id},#{modelName},#{action},#{description},#{ip},#{createTime},#{success},#{message},#{userId},#{className},#{methodName},#{methodArgs})
    </insert>
</mapper>